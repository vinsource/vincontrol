                                        <TextBlock Text="Dealer Info"></TextBlock>
                                        <TextBox></TextBox>
                                        <TextBlock Text="Dealer Name"></TextBlock>
                                        <TextBox></TextBox>
                                        <TextBlock Text="Dealer Address"></TextBlock>
                                        <TextBox></TextBox>
