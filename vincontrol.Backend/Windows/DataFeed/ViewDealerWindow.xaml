<Window x:Class="vincontrol.Backend.Windows.DataFeed.ViewDealerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterScreen"
        Title="ViewDealerWindow" Height="700" Width="1000">
    <Grid>
        <StackPanel Orientation="Vertical">
            <DataGrid CanUserAddRows="False" AutoGenerateColumns="False" Name="dataGrid1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10">
                <DataGrid.Columns>
                    <DataGridHyperlinkColumn Header="Dealer Name" Width="200"></DataGridHyperlinkColumn>
                    <DataGridTextColumn Header="Remove"></DataGridTextColumn>
                </DataGrid.Columns>
            </DataGrid>
            <Button Content="Remove selected Dealer From Profile" Width="300"></Button>
            <Button Content="Add Dealer To Profile" Width="200"></Button>
        </StackPanel>
    </Grid>
</Window>
