<Window x:Class="vincontrol.Backend.Windows.DataFeed.AddDealerGroupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterScreen"
        Title="Add Dealer Group" Height="700" Width="1000">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Text="Dealer Group ID"></TextBlock>
            <TextBox Name="txtID"></TextBox>
            <TextBlock Text="Dealer Group Name"></TextBlock>
            <TextBox Name="txtName"></TextBox>
            <TextBlock Text="Master User Name"></TextBlock>
            <TextBox Name="txtUserName"></TextBox>
            <TextBlock Text="Master Login"></TextBlock>
            <TextBox  Name="txtMasterlogin"></TextBox>
            <TextBlock Text="Default Dealer ID"></TextBlock>
            <TextBox Name="txtDefaultDealerID"></TextBox>
            <Button Width="100" HorizontalAlignment="Left" Name="btnCreateDealerGroup" Click="btnCreateDealerGroup_Click">Create</Button>
        </StackPanel>
    </Grid>
</Window>
